{
  "name": "wp-property/wp-property-walkscore",
  "type": "wordpress-plugin",
  "minimum-stability": "dev",
  "homepage": "https://www.usabilitydynamics.com/product/wp-property-walkscore",
  "author": {
    "name": "UsabilityDynamics, Inc.",
    "url": "https://www.usabilitydynamics.com"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://repository.usabilitydynamics.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wp-property/wp-property-walkscore.git"
  },
  "require": {
    "php": ">=5.3",
    "composer/installers": "~1.0",
    "usabilitydynamics/lib-wp-bootstrap": "1.1.3"
  },
  "require-dev": {
    "usabilitydynamics/lib-wp-phpunit": "~0.2",
    "codeclimate/php-test-reporter": "~0.1"
  },
  "autoload": {
    "classmap": [ "lib/classes" ]
  },
  "archive": {
    "exclude": [
      "vendor",
      "node_modules",
      ".gitignore",
      "test",
      "circle.yml",
      "composer.lock",
      "gruntfile.js",
      "package.json"
    ]
  },
  "extra": {
    "schemas": {
      "dependencies" : {
        "modules": {},
        "plugins": [
          {
            "name": "WP-Property",
            "slug": "wp-property",
            "version": "2.0.2",
            "required": true
          }
        ]
      },
      "licenses" : {
        "client" : false,
        "product" : false
      },
      "ui": {
        "configuration": {
          "secondary_menu": {
            "page_title": "l10n.settings_page_title",
            "menu_title": "l10n.walkscore",
            "parent_slug": "edit.php?post_type=property",
            "menu_slug": "walkscore"
          }
        },
        "menu": {
          "general": {
            "id": "general",
            "name": "l10n.general",
            "desc": "l10n.general_settings"
          }
        },
        "sections": {
          "api": {
            "id": "api",
            "name": "l10n.api_settings",
            "menu": "general"
          }
        },
        "fields": [
          {
            "id": "config.api.key",
            "name": "l10n.api_key",
            "desc": "l10n.desc_api_key",
            "section": "api"
          }
        ]
      }
    }
  }
}