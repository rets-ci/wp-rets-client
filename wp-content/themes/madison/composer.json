{
  "name": "wp-property/wp-madison",
  "type": "wordpress-theme",
  "minimum-stability": "dev",
  "homepage": "https://www.usabilitydynamics.com/product/wp-madison/",
  "authors": [
    {
      "name": "Justin Kopepasah",
      "homepage": "http://kopepasah.com",
      "role": "Developer"
    },
    {
      "name": "UsabilityDynamics, Inc.",
      "email": "info@usabilitydynamics.com",
      "homepage": "https://www.usabilitydynamics.com",
      "role": "Developer"
    }
  ],
  "keywords": [
    "real estate",
    "property management",
    "modern",
    "customizer",
    "custom header",
    "wp-property"
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "http://repository.usabilitydynamics.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wp-property/wp-madison.git"
  },
  "require": {
    "php": ">=5.3",
    "usabilitydynamics/lib-utility": "0.3.6",
    "usabilitydynamics/lib-wp-bootstrap": "1.1.9",
    "usabilitydynamics/lib-ud-api-client": "1.1.4"
  },
  "require-dev": {
    "usabilitydynamics/lib-wp-phpunit": "~0.2",
    "codeclimate/php-test-reporter": "~0.1"
  },
  "autoload": {
    "classmap": [ "lib/classes" ]
  },
  "archive": {
    "exclude": [
      "vendor",
      "node_modules",
      ".gitignore",
      "test",
      "circle.yml",
      "composer.lock",
      "gruntfile.js",
      "package.json"
    ]
  },
  "extra": {
    "schemas": {
      "dependencies" : {
        "modules": {},
        "plugins": [
          {
            "name": "WP-Property",
            "slug": "wp-property",
            "version": "2.0.0",
            "required": false
          },
          {
            "name": "Easy Google Fonts",
            "slug": "easy-google-fonts",
            "required": false
          },
          {
            "name": "Breadcrumb Trail",
            "slug": "breadcrumb-trail",
            "required": false
          },
          {
            "name": "Jetpack",
            "slug": "jetpack",
            "required": false
          }
        ]
      },
      "licenses" : {
        "client" : {
          "slug": "wp-property/wp-madison",
          "screen": {
            "parent": "themes.php",
            "page_title": "l10n.licenses_page_title",
            "menu_title": "l10n.licenses_menu_title"
          }
        },
        "product" : {
          "referrer": false,
          "product_id": "wp-property/wp-madison"
        }
      }
    }
  }
}