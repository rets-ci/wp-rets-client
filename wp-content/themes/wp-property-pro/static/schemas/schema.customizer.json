  "settings": [
    {
      "key": "property_pro_primary_color",
      "label": "Primary Color",
      "section": "property_pro_colors_section",
      "control": "color",
      "important": false,
      "default": "#000000",
      "selector": [
        "#menu-item"
      ]
    },
    {
      "key": "property_pro_secondary_color",
      "label": "Secondary Color",
      "section": "property_pro_colors_section",
      "control": "color",
      "important": false,
      "default": "#000000",
      "selector": [
        "#selector"
      ]
    },
    {
      "key": "property_pro_heading_font",
      "label": "Heading Font-Family",
      "section": "property_pro_fonts_section",
      "control": "select",
      "important": false,
      "default": "0",
      "choices": {
        "0": "Make a choice",
        "Arial, Helvetica, sans-serif": "Arial, Helvetica, sans-serif",
        "\"Arial Black\", Gadget, sans-serif": "\"Arial Black\", Gadget, sans-serif",
