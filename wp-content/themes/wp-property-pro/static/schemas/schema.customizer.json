      "rule-title": "font-family",
      "selector": [
        ".pageheader p.hidden-sm-down",
        ".testimonial .sliderContent blockquote p",
        ".testimonial .userInfo .userBox p",
        ".testimonial .userInfo .userBox span"
      ]
    },
    {
      "key": "property_pro_brand_type",
