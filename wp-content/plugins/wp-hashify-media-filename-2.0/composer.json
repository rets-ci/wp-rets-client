
{
  "name": "usabilitydynamics/wp-hashify-media-filename",
  "type": "wordpress-plugin",
  "description": "WordPress plugin that randomizes the filename of newly uploaded media files.",
  "homepage": "https://github.com/usabilitydynamics/wp-hashify-media-filename",
  "keywords": [
    "wordpress",
    "multisite",
    "random"
  ],
  "license": "MIT",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Eric Sopp",
      "homepage": "http://usabilitydynamics.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "http://repository.usabilitydynamics.com"
    }
  ],
  "config": {
    "preferred-install": "dist",
    "github-protocols": [ "ssh" ],
    "vendor-dir": "vendor",
    "bin-dir": "vendor/bin",
    "optimize-autoloader": true
  },
  "require": {
    "php": ">=5.3",
    "composer/installers": "~1.0"
  }
}
